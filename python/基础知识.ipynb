   "source": [
    "#### 1. str 和 bytes转换\n",
    "    - 编码、解码的方式必须一样"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "b'asd'\n",
      "asd\n"
     ]
    }
   ],
   "source": [
    "# str to bytes\n",
    "b = 'asd'.encode()\n",
    "print(b)\n",
    "\n",
    "# bytes to str\n",
    "c = b.decode()\n",
    "print(c)\n"
   ]
  },
